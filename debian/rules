#!/usr/bin/make -f

#export DH_VERBOSE=1

binary binary-arch binary-indep build clean:
	dh $@ --with epics -Sepicsmake
install: build
binary-arch binary-indep: install
binary: binary-arch binary-indep

install: build install-stamp
install-stamp:
	dh install --until install --with epics -Sepicsmake

	dh install --after install --with epics -Sepicsmake
	touch $@
